{
    "name" : "keosu-menu",
    "description" : "Create a menu gadget",
    "version" : "0.2",
    "type" : "gadget",
    "param" : [
		{ "name" : "menu-id", "type" : "number" }
    ],
    "configCordova" : [
        {
            "plugin": [
                {
                    "@attributes": {
                        "name": "cordova-plugin-inappbrowser",
                        "spec": "1.2.1",
                        "source": "npm"
                    }
                }
            ]
        }
    ],
    "pluginToInstall" : [
        "cordova plugin add cordova-plugin-inappbrowser"
    ],
    "require" : [
        { "name" : "keosu-base" , "version" : ">=0.1" }
    ]
}
