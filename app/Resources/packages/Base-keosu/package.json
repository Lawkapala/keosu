{
    "name" : "keosu-base",
    "description" : "contains all the required package and template for keosu",
    "version" : "0.1",
    "type" : "base",
	"configCordova" : [
		{ "plugin" :[
			{ "@attributes" : {"name" : "cordova-plugin-device" } }
		]},
		{ "plugin" :[
			{ "@attributes" : {"name" : "cordova-plugin-device-orientation" } }
		]},
		{ "plugin" :[
			{ "@attributes" : {"name" : "cordova-plugin-dialogs" } }
		]},
		{ "plugin" :[
			{ "@attributes" : {"name" : "cordova-plugin-network-information" } }
		]},
		{ "plugin" : [
			{ "@attributes" : {"name" : "cordova-plugin-whitelist"} }
		]}
	],

"pluginToInstall" : [
		"cordova plugin add cordova-plugin-device",
		"cordova plugin add cordova-plugin-device-orientation",
		"cordova plugin add cordova-plugin-dialogs",
		"cordova plugin add cordova-plugin-network-information",
		"cordova plugin add cordova-plugin-whitelist"
	],
    "require" : [
        { "name" : "angularJs" , "version" : ">=1.2" },
        { "name" : "jQuery", "version" : ">=2" },
        { "name" : "keosu-push" , "version" : ">=0.1"}
    ]
}
